<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>com.dgraf.nashornplayground.HelloWorld</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddingJava</servlet-name>
        <servlet-class>com.dgraf.nashornplayground.AddingJava</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bindings</servlet-name>
        <servlet-class>com.dgraf.nashornplayground.Bindings</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Performance</servlet-name>
        <servlet-class>com.dgraf.nashornplayground.Performance</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddingJava</servlet-name>
        <url-pattern>/AddingJava</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bindings</servlet-name>
        <url-pattern>/Bindings</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Performance</servlet-name>
        <url-pattern>/Performance</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
